{
    "name": "label84/laravel-active-campaign",
    "description": "Add Active Campaign to your Laravel application.",
    "type": "library",
    "license": "MIT",
    "authors": [
      {
        "name": "Label84",
        "email": "info@label84.com"
      }
    ],    
    "require": {
      "php": "^8.0.2",
      "illuminate/support": "^8.0|^9.0"
    },
    "autoload": {
      "psr-4": {
          "Label84\\ActiveCampaign\\": "src"
      }
    },
    "extra": {
      "laravel": {
          "providers": [
              "Label84\\ActiveCampaign\\ActiveCampaignServiceProvider"
          ],
          "aliases": {
            "StatsHelper": "Label84\\ActiveCampaign\\Facades\\ActiveCampaign"
        }
      }
    },
    "require-dev": {
        "orchestra/testbench": "^6.0|7.0",
        "nunomaduro/larastan": "^2.0",
        "phpunit/phpunit": "^9.5"
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
